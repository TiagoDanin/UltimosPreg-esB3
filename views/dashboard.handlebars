<div class="uk-grid-column-small uk-grid-row-large uk-child-width-2-2@s uk-text-center" uk-grid>
	<form action="/" method="post">
		<div class="uk-margin" uk-grid>

			<span class="uk-text-bold">ID Ação</span>
			<div class="uk-inline uk-width-1-1">
				<span class="uk-form-icon uk-form-icon-flip" uk-icon="icon: search"></span>
				<input class="uk-input" name="id" type="query">
			</div>

			<div class="uk-width-1-1">
				<button class="uk-button uk-button-default uk-button-text" type="submit" name="ok"
					value="true">Obter</button>
			</div>
		</div>
	</form>

	{{#forEach result}}
	<div class="uk-card uk-card-default uk-card-body" id="card-query">
		<p class="uk-card-title uk-margin-remove">{{getId}}</p>
	</div>
	{{/forEach}}
</div>

